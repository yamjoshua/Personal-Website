{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/no-array-index-key": "off"
  },
  "parser": "babel-eslint",
  "plugins": ["react", "jsx-a11y", "import"],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  }
}
